    "apache_http" : {
      "port" : "80",
      "directives" : {
        "mode" : "http",
        "balance" : "roundrobin",
        "cookie" : [ 
          "SERVERID", 
          "insert", 
          "indirect" 
        ]
      },      
      "options" : {
        "httpclose" : "",
        "forwardfor" : "",
        "httpchk" : [ 
          "HEAD", 
          "/index.php", 
          "HTTP/1.0" 
        ]
      },
      "servers" : {
        "web1" : {
          "ip" : "127.0.0.1",
          "port" : "160",
          "settings" : [
            [ "port", "160" ],
            [ "cookie", "web1" ],
            "check"
          ]        
        }   
      }
    },
    "apache_https" : {
      "port" : "443",
      "directives" : {
        "mode" : "http",
        "balance" : "roundrobin",
        "cookie" : [ 
          "SERVERID", 
          "insert", 
          "indirect" 
        ]
      }, 
      "options" : {
        "httpclose" : "",
        "forwardfor" : "",
        "httpchk" : [ 
          "HEAD", 
          "/index.php", 
          "HTTP/1.0" 
        ]
      },
      "servers" : {
        "secure1" : {
          "ip" : "127.0.0.1",
          "port" : "886",
          "settings" : [
            [ "port", "886" ],
            [ "cookie", "secure1" ],
            "check"
          ]        
        }   
      }
    },
    "mysql_write" : {
      "port" : "3312",
      "directives" : {
        "mode" : "tcp",
        "balance" : "leastconn"
      }, 
      "options" : [
        "httpchk"
      ],
      "servers" : {
        "db1" : {
          "ip" : "127.0.0.1",
          "port" : "6612",
          "settings" : [
            "check",
            [ "port", "9200"  ],
            [ "inter", "12000" ],
            [ "rise", "3" ],
            [ "fall", "3" ]
          ]        
        },
        "db2" : {
          "ip" : "127.0.0.1",
          "port" : "6612",
          "settings" : [
            "check",
            [ "port", "9200"  ],
            [ "inter", "12000" ],
            [ "rise", "3" ],
            [ "fall", "3" ],
            "backup"
          ]        
        }      
      }
    },
    "mysql_read" : {
      "port" : "3306",
      "directives" : {
        "mode" : "tcp",
        "balance" : "leastconn"
      },
      "options" : [
        "httpchk"
      ],
      "servers" : {
        "db1" : {
          "ip" : "127.0.0.1",
          "port" : "6612",
          "settings" : [
            "check",
            [ "port", "9200"  ],
